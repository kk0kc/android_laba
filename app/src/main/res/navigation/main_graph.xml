<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_graph"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.example.androidlab.firstlevel.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" />
    <fragment
        android:id="@+id/secondFragment"
        android:name="com.example.androidlab.firstlevel.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" />
    <include app:graph="@navigation/nested_graph" />

    <fragment
        android:id="@+id/fourthFragment"
        android:name="com.example.androidlab.firstlevel.FourthFragment"
        android:label="fragment_fourth"
        tools:layout="@layout/fragment_fourth"/>
    <fragment
        android:id="@+id/fifthFragment"
        android:name="com.example.androidlab.firstlevel.FifthFragment"
        android:label="fragment_fifth"
        tools:layout="@layout/fragment_fifth" />

</navigation>